{
  "name": "cdk-dynamo-table-viewer",
  "version": "3.1.2",
  "awscdkio": { 
    "twitter": "emeshbi" 
  },
  "scripts": {
    "build": "jsii",
    "watch": "jsii -w",
    "package": "jsii-pacmak -vv",
    "test": "jest",
    "cdk": "cdk"
  },
  "keywords": [ "cdk", "dynamodb", "sample" ],  
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "short",
    "targets": {
      "java": {
        "package": "com.github.eladb.dynamotableviewer",
        "maven": {
          "groupId": "com.github.eladb",
          "artifactId": "cdk-dynamo-table-viewer"
        }
      },
      "python": {
        "distName": "cdk-dynamo-table-viewer",
        "module": "cdk_dynamo_table_viewer"
      },
      "dotnet": {
        "namespace": "Eladb.DynamoTableViewer",
        "packageId": "Eladb.DynamoTableViewer",
        "signAssembly": false
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ]
  },
  "author": {
    "email": "elad.benisrael@gmail.com",
    "name": "Elad Ben-Israel",
    "url": "https://github.com/eladb"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eladb/cdk-dynamo-table-viewer.git"
  },
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@types/node": "13.1.7",
    "jest": "^24.9.0",
    "jsii": "^0.21.1",
    "jsii-pacmak": "^0.21.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.20.0",
    "@aws-cdk/aws-dynamodb": "^1.20.0",
    "@aws-cdk/aws-lambda": "^1.20.0",
    "@aws-cdk/core": "^1.20.0"
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigateway": "^1.20.0",
    "@aws-cdk/aws-dynamodb": "^1.20.0",
    "@aws-cdk/aws-lambda": "^1.20.0",
    "@aws-cdk/core": "^1.20.0"
  },
  "stability": "experimental"
}
